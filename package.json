    "license": "MIT",
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20260214.0",
        "@types/js-yaml": "^4.0.9",
        "@types/node": "^24.0.0",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3",
        "wrangler": "^4.65.0"
    },
    "dependencies": {
        "hono": "^4.11.9",
        "js-yaml": "^4.1.1"
    }
}